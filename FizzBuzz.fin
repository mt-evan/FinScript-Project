for (i <- 1 to 100) {

    if (i == 13) {
        println("Continuing at 13")
        continue
    } 

    if (i == 98) {
        println("Breaking at 98")
        break
    }


    if (i % 15 == 0) {
        println("FizzBuzz")
    } elif (i % 3 == 0) {
        println("Fizz")
    } elif (i % 5 == 0) {
        println("Buzz") 
    } else {
        println(i)
    }
}