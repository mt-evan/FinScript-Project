for (i <- 1 to 30) {

    if (i == 13) {
        println("Continuing at 13")
        continue
    } 

    if (i == 28) {
        println("Breaking at 28")
        break
    }


    if (i % 5 == 0 && i % 3 == 0) {
        println("FizzBuzz")
    } elif (i % 3 == 0) {
        println("Fizz")
    } elif (i % 5 == 0) {
        println("Buzz") 
    } else {
        println(i)
    }
}