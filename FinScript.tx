Program: 
    statements*=Statement
;

Statement:
    Output | Declaration
;

Output:
    OutputVar | OutputValue
;

OutputVar:
    'print' '(' content=ID ')' 
;

OutputValue:
    'print' '(' content=Value ')' 
;

Declaration:
    'let' name=ID '=' value=Value
;

Value:
    STRING | NUMBER | FLOAT | BOOL
;

Comment:
    /\/\/.*$/
;
